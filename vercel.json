{
  "version": 2,
  "builds": [
    {
      "src": "build/**/*.+(js|jsx|ts|tsx)",
      "use": "@vercel/static-build",
      "config": {
        "cleanUrls": true
      }
    }
  ],
  "routes": [
    { "src": "/(favicon.ico|robots.txt)", "dest": "/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
